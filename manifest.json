{
  "manifest_version": 3,
  "name": "Dev.Home",
  "version": "1.3",
  "description": "For college devs: organize contests, hackathons, and bookmarks. Open-source, contribute and add features!",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["bookmarks"],
  "host_permissions": ["https://chrome-home-server.vercel.app/*"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
